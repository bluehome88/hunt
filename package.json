{
  "name": "hunt",
  "version": "0.2.0",
  "description": "High level nodejs framework build on top of expressjs, mongoose, sequilize, socketio and passportjs",
  "main": "index.js",
  "engines": {
    "node": ">=0.10.29",
    "npm": ">=1.4.3"
  },
  "os": [ "linux", "darwin" ],
  "scripts": {
    "start": "node examples/fullExample.js",
    "test": "./node_modules/.bin/mocha --reporter spec; ./generateDocs.sh",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha"
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:vodolaz095/hunt.git"
  },
  "directories": {
    "example": "./examples",
    "doc": "./examples/views/documentation",
    "lib": "./lib",
    "test": "./test"
  },
  "bugs": {
    "url": "https://bitbucket.org/vodolaz095/hunt/issues"
  },
  "keywords": [
    "hunt",
    "boilerplate",
    "framework",
    "express",
    "mongoose",
    "sequilize",
    "socket.io",
    "passportjs"
  ],
  "author": {
    "name": "Anatolij Ostroumov",
    "url": "https://github.com/vodolaz095",
    "email": "ostroumov095@gmail.com"
  },
  "homepage": "https://huntjs.herokuapp.com/",
  "license": "MIT",
  "dependencies": {
    "async": ">=0.9.0",
    "validator": ">=3.0.0",
    "express": ">=4.6.1",
    "body-parser": ">=1.5.0",
    "connect-multiparty": ">=1.0.5",
    "compression": ">=1.0.9",
    "serve-favicon": ">=2.0.0",
    "cookie-parser": ">=1.0.0",
    "method-override": ">=1.0.0",
    "express-session": ">=1.0.0",
    "response-time": ">=1.0.0",
    "morgan": ">=1.0.1",
    "errorhandler": ">=1.0.1",
    "csurf": ">=1.1.0",
    "express-resource": "~1.0.0",
    "hogan-express": "~0.5.0",
    "jade": ">=1.4.1",
    "swig": ">=1.2.2",
    "redis": ">=0.10.0",
    "connect-redis": ">=2.0.0",
    "connect-flash": "*",
    "socket.io": "0.9.17",
    "passport.socketio": ">=3.0.0",
    "passport": "*",
    "passport-local": "1.0.0",
    "passport-hash": "*",
    "passport-google": "0.3.0",
    "passport-github": "0.1.5",
    "passport-twitter": "*",
    "passport-facebook": "*",
    "passport-vkontakte": "*",
    "passport-yahoo": "*",
    "passport-intuit": "*",
    "passport-paypal": "*",
    "passport-steam": "*",
    "mongoose": "3.8.12",
    "colors": "*",
    "nodemailer": "0.7.1",
    "rai": "<0.2.0",
    "request": "latest"
  },
  "optionalDependencies": {
    "hiredis": "*"
  },
  "devDependencies": {
    "mocha": ">=1.20.0",
    "istanbul": "*",
    "should": "3.x",
    "jsdoc": "*"
  }
}
