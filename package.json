{
  "name": "hunt",
  "version": "0.2.3",
  "description": "High level nodejs framework build on top of expressjs, mongoose, sequilize, socketio and passportjs",
  "main": "index.js",
  "engines": {
    "node": ">=0.10.32",
    "npm": ">=1.4.3"
  },
  "os": [ "linux", "darwin" ],
  "scripts": {
    "start": "node examples/index.js",
    "test": "./node_modules/.bin/mocha --reporter spec; ./generateDocs.sh",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha"
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:vodolaz095/hunt.git"
  },
  "directories": {
    "example": "./examples",
    "doc": "./examples/public/documentation",
    "lib": "./lib",
    "test": "./test"
  },
  "bugs": {
    "url": "https://bitbucket.org/vodolaz095/hunt/issues"
  },
  "keywords": [
    "hunt",
    "boilerplate",
    "framework",
    "express",
    "mongoose",
    "sequilize",
    "socket.io",
    "passportjs"
  ],
  "author": {
    "name": "Anatolij Ostroumov",
    "url": "https://github.com/vodolaz095",
    "email": "ostroumov095@gmail.com"
  },
  "homepage": "https://huntjs.herokuapp.com/",
  "license": "MIT",
  "dependencies": {
    "async": "0.9.0",
    "eventemitter2": "0.4.14",
    "validator": "3.19.0",
    "express": "4.9.4",
    "body-parser": "1.9.0",
    "connect-multiparty": "1.2.4",
    "compression": "1.1.0",
    "serve-favicon": "2.1.4",
    "cookie-parser": "1.3.3",
    "method-override": "2.2.0",
    "express-session": "1.8.2",
    "response-time": "2.2.0",
    "morgan": "1.3.1",
    "errorhandler": "1.2.0",
    "csurf": "1.6.1",
    "hogan-express": "0.5.2",
    "jade": "1.7.0",
    "swig": "1.4.2",
    "express-handlebars":"1.1.0",
    "redis": "0.12.1",
    "connect-redis": "2.1.0",
    "connect-flash": "0.1.1",
    "socket.io": "1.1.0",
    "socket.io-redis":"0.1.3",
    "passport.socketio": "3.4.1",
    "passport": "0.2.1",
    "passport-local": "1.0.0",
    "passport-hash": "0.3.0",
    "passport-google": "0.3.0",
    "passport-github": "0.1.5",
    "passport-twitter": "1.0.2",
    "passport-facebook": "1.0.3",
    "passport-vkontakte": "0.3.0",
    "passport-yahoo": "0.3.0",
    "passport-intuit": "0.2.0",
    "passport-paypal": "0.3.0",
    "passport-steam": "0.1.4",
    "mongoose": "3.8.16",
    "colors": "0.6.2",
    "nodemailer": "1.3.0",
    "nodemailer-html-to-text":"1.0.0",
    "rai": "0.1.11",
    "request": "2.44.0"
  },
  "optionalDependencies": {
    "hiredis": "0.1.17"
  },
  "devDependencies": {
    "mocha": "1.21.4",
    "istanbul": "0.3.2",
    "should": "3.3.2",
    "jsdoc": "3.3.0-alpha9"
  }
}
