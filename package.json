{
  "name": "hunt",
  "version": "0.7.2",
  "description": "High level nodejs framework build on top of expressjs, mongoose, sequilize, socketio and passportjs",
  "main": "index.js",
  "engines": {
    "node": ">=6.8.0",
    "npm": ">=4.1.2"
  },
  "os": [
    "linux",
    "darwin"
  ],
  "scripts": {
    "start": "node examples/index.js",
    "test": "mocha --reporter spec ./test/*.test.js",
    "coverage": "istanbul cover ./node_modules/.bin/_mocha",
    "testReport": "mocha -G --reporter markdown > tutorials/test.md",
    "frontend": "gulp",
    "install": "./postinstall.sh",
    "jslint": "jslint **/* --config jslint.conf",
    "jshint": "jshint --verbose --show-non-errors -c .jshintrc .",
    "docs": "make docs"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:vodolaz095/hunt.git"
  },
  "directories": {
    "example": "./examples",
    "doc": "./examples/public/documentation",
    "lib": "./lib",
    "test": "./test"
  },
  "bugs": {
    "url": "https://github.com/vodolaz095/hunt/issues"
  },
  "keywords": [
    "hunt",
    "boilerplate",
    "framework",
    "express",
    "mongoose",
    "sequilize",
    "socket.io",
    "passportjs"
  ],
  "author": {
    "name": "Anatolij Ostroumov",
    "url": "https://github.com/vodolaz095",
    "email": "ostroumov095@gmail.com"
  },
  "homepage": "https://huntjs.herokuapp.com/",
  "license": "MIT",
  "dependencies": {
    "async": "^2.1.5",
    "body-parser": "^1.16.1",
    "compression": "^1.6.2",
    "connect-flash": "^0.1.1",
    "connect-multiparty": "^2.0.0",
    "connect-redis": "^3.2.0",
    "cookie-parser": "^1.4.3",
    "csurf": "^1.9.0",
    "dev": "^0.1.3",
    "errorhandler": "^1.5.0",
    "eventemitter2": "^3.0.1",
    "express": "^4.14.1",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.1",
    "glob": "^7.1.1",
    "gulp-debug": "^3.1.0",
    "hogan-express": "0.5.2",
    "jade": "^1.11.0",
    "kerberos": "^0.0.22",
    "method-override": "^2.3.7",
    "mongoose": "^4.8.4",
    "nodemailer": "^3.1.3",
    "nodemailer-direct-transport": "^3.3.2",
    "nodemailer-html-to-text": "^2.1.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-github": "^1.1.0",
    "passport-google-oauth": "^1.0.0",
    "passport-steam": "^1.0.8",
    "passport-twitter": "^1.0.4",
    "passport-vkontakte": "^0.4.0",
    "pug": "^2.0.0-beta11",
    "q": "^1.4.1",
    "rai": "^0.1.12",
    "redis": "^2.6.5",
    "request": "^2.79.0",
    "response-time": "^2.3.2",
    "sequelize": "^3.30.2",
    "serve-favicon": "^2.4.0",
    "socket.io": "^1.7.3",
    "socket.io-redis": "^4.0.0",
    "swig": "1.4.2",
    "validator": "^7.0.0",
    "winston": "^2.1.1"
  },
  "optionalDependencies": {
    "hiredis": "^0.5.0"
  },
  "devDependencies": {
    "bower": ">=1.7.2",
    "del": ">=2.0.0",
    "istanbul": ">=0.4.2",
    "mocha": ">=2.3.4",
    "jsdoc": ">=3.4.0",
    "jslint": ">=0.9.3",
    "jshint": ">=2.9.1",
    "should": "^8.1.1",
    "socket.io-client": "^1.7.3",
    "sqlite3": "^3.1.1",
    "gulp": ">=3.8.11",
    "gulp-bower": ">=0.0.10",
    "gulp-concat": ">=2.6.0",
    "gulp-minify-css": ">=1.2.1",
    "gulp-less": "^3.0.5",
    "gulp-order": "^1.1.1",
    "gulp-rename": ">=1.2.2",
    "gulp-uglify": ">=1.4.0"
  }
}
