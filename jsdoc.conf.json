{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": [
      "README.md",
      "index.js",
      "lib/api/",
      "lib/datastore",
      "lib/exportModel",
      "lib/exportModel/lib/toRest",
      "lib/http",
      "lib/misc",
      "lib/telnet",
      "lib/models/",
      "lib/passportStrategies/",
      "lib/templateEngines/"
    ],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "default": {
      "outputSourceFiles": false
    }
  },
  "opts": {
    "destination": "./examples/public/documentation"
  }
}